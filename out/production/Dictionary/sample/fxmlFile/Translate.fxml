<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TranslatorController">
   <children>
      <TextField fx:id="textFrom" alignment="TOP_LEFT" layoutX="35.0" layoutY="110.0" prefHeight="90.0" prefWidth="520.0" promptText="Type to translate" />
      <TextField fx:id="textTo" alignment="TOP_LEFT" layoutX="35.0" layoutY="224.0" prefHeight="90.0" prefWidth="520.0" promptText="Translation" />
      <Label layoutX="237.0" layoutY="36.0" prefHeight="21.0" prefWidth="158.0" text="Translate">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button layoutX="158.0" layoutY="334.0" mnemonicParsing="false" onAction="#trans" text="Submit" />
      <Button layoutX="363.0" layoutY="334.0" mnemonicParsing="false" onAction="#Cancel" text="Cancel" />
      <Button fx:id="button" layoutX="427.0" layoutY="68.0" mnemonicParsing="false" onAction="#changeStatus" text="E-V" />
   </children>
</AnchorPane>
